{
	"name": "frotaweb",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"prebuild-traccar": "./prebuild.sh",
		"build-traccar": "cd traccar-web && npm install && npm run generate-pwa-assets && npm run build",
		"prebuild": "npm run build-traccar",
		"build": "vite build && rm -rf output && mkdir output && mv build output/main && mv traccar-web/build output/map",
		"postbuild": "cp -v _redirects output",
		"preview": "vite preview",
		"lint": "eslint ."
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@types/eslint": "^9.6.0",
		"autoprefixer": "^10.4.20",
		"eslint": "^9.7.0",
		"eslint-plugin-svelte": "^2.36.0",
		"globals": "^15.0.0",
		"svelte": "^5.0.0",
		"tailwindcss": "^3.4.9",
		"vite": "^5.0.3"
	},
	"dependencies": {
		"@sveltejs/adapter-static": "^3.0.6",
		"@unovis/svelte": "^1.4.4",
		"@unovis/ts": "^1.4.4"
	}
}
